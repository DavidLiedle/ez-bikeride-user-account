<tr>
    <td>
        <strong>
            <a href="{{ path( "ez_urlalias", { 'locationId': content.contentInfo.mainLocationId } ) }}"
               target="_self">{{ ez_content_name( content ) }}</a>
        </strong>
    </td>
    <td>
        {{
            ez_render_field( content,
                             'starting_point',
                             {
                                'parameters': {
                                    'width':    '100%',
                                    'height':   '100px',
                                    'showMap':  true,
                                    'showInfo': true
                                }
                             }
                           )
        }}
    </td>
    <td>
        {{
            ez_render_field( content,
                             'ending_point',
                             {
                                'parameters': {
                                    'width': '100%',
                                    'height': '100px',
                                    'showMap': true,
                                    'showInfo': true
                                }
                             }
                           )
      }}
    </td>
    <td>
        <p>
          {{ ez_render_field( content, 'length' ) }} Km
        </p>
    </td>
    <td>
        <p>
          {{ ez_render_field( content, 'level' ) }}
        </p>
    </td>
</tr>
